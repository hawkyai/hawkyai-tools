{
  "buildCommand": "pnpm vercel-build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/compliance-checker/api/:match*",
      "destination": "/compliance-checker/api/:match*"
    },
    {
      "source": "/compliance-checker",
      "destination": "/compliance-checker/"
    },
    {
      "source": "/compliance-checker/:path*",
      "destination": "/compliance-checker/:path*"
    },
    {
      "source": "/",
      "destination": "https://hawky.framer.website/"
    },
    {
      "source": "/((?!compliance-checker).)*",
      "destination": "https://hawky.framer.website/$1"
    }
  ]
}
